# OpenClaw Dashboard â€” Environment Variables
# Copy this file to .env and adjust values for your setup.

# Path to your OpenClaw installation directory
OPENCLAW_DASH_OPENCLAW_DIR=/home/your-user/.openclaw

# Gateway URL (for AI chat proxy)
OPENCLAW_DASH_GATEWAY_URL=http://localhost:18789
OPENCLAW_DASH_GATEWAY_WS_URL=ws://127.0.0.1:18789
OPENCLAW_DASH_GATEWAY_TOKEN=

# Server settings
OPENCLAW_DASH_HOST=0.0.0.0
OPENCLAW_DASH_PORT=8765
OPENCLAW_DASH_LOG_LEVEL=info

# How often (seconds) the discovery engine re-scans your workspace
OPENCLAW_DASH_DISCOVERY_INTERVAL_SECONDS=300
